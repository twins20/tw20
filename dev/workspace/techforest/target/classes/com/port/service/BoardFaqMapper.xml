<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.port.service.BoardFaqMapper">

 	<select id="boardFaqList" resultType="com.port.service.BoardVo" >
 	<![CDATA[	   
   	    SELECT 
   	    	*
   	    FROM 
   	    	TF_BOARD_FAQ 
		ORDER 
			BY BIDX DESC 	
	]]>    
    </select>
       			
	<select id="boardFaqCon" resultType="com.port.service.BoardVo" parameterType="int" >
	<![CDATA[	
		SELECT 
			* 
		FROM 
			TF_BOARD_FAQ
		WHERE 
			BIDX = #{bidx}
	]]>
	</select>	

</mapper>
 


